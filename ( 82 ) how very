<script>
    $(document).scroll(function() {

        myID = document.getElementById("pagination");

        var myScrollFunc = function () {
            var y = window.scrollY;
            if (y >= 300) {
                myID.className = "pagination show"
            } else {
                myID.className = "pagination hide"
            }
        };

        window.addEventListener("scroll", myScrollFunc);
    });
</script>
