<script>
    $(document).ready(function() {      
    $('#pop').click(function() {
        $('#pop').fadeOut();
        $('#sidebar').fadeOut();
        $('.posts').fadeOut();
        $('.popup').fadeIn();
        
    });
    
    $('.popup .close').click(function() {
        $('.popup').fadeOut();
        $('.posts').fadeIn();
        $('#sidebar').fadeIn();
        $('#pop').fadeIn();
    });
 });  
</script>

