<script>
$(document).ready(function() {
    $('#trigger').click(function() {
        $('#container').fadeOut();
        $('.popup').fadeIn();
    });
    
    $('.popup .close').click(function() {
        $('.popup').fadeOut();
        $('#container').fadeIn();
    });
});
</script>

        
