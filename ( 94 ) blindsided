<script>
$(document).ready(function() {
    $('#muses').click(function() {
        $('#popup').fadeIn();
        $('.close').fadeIn();
        $('section').fadeOut();
    });
    
    $('.close').click(function() {
        $('#popup').fadeOut();
        $('.close').fadeOut();
        $('section').fadeIn();
    });
});
</script>

<script>
    $(document).ready(function(){
      $('#daynight').click(function(){
         $('body').toggleClass('night');
         $('#daynight span').toggleClass('fa-moon');
      });
   });
</script>
