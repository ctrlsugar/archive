<script>
$(document).ready(function() {
    $('#rnav').click(function() {
        $('#guidelines').fadeIn();
        $('.close').fadeIn();
        $('.posts').fadeOut();
         $('ol.notes').fadeOut();
    });
    
    $('.close').click(function() {
        $('#guidelines').fadeOut();
        $('.close').fadeOut();
        $('.posts').fadeIn();
        $('ol.notes').fadeIn();
    });
});
</script>

<script>
$(document).ready(function() { 
    $('#mnav').click(function() {
        $('#muses').fadeIn();
        $('.exit').fadeIn();
        $('.posts').fadeOut();
        $('ol.notes').fadeOut();
    });
    
    $('.exit').click(function() {
        $('#muses').fadeOut();
        $('.exit').fadeOut();
        $('.posts').fadeIn();
        $('ol.notes').fadeIn();
    });
});
</script>

<script>
$(document).ready(function() { 
    $('#snav').click(function() {
        $('#single').fadeIn();
        $('.bye').fadeIn();
        $('.posts').fadeOut();
    });
    
    $('.bye').click(function() {
        $('#single').fadeOut();
        $('.bye').fadeOut();
        $('.posts').fadeIn();
    });
});
</script>

