<script>
$(document).ready(function() {
    $('#muses').click(function() {
        $('.posts').fadeOut();
        $('.tagsnav').fadeOut();
        $('.muses').fadeIn();
    });
    
    $('.muses .close').click(function() {
        $('.muses').fadeOut();
        $('.posts').fadeIn();
    });
});
</script>

<script>
$(document).ready(function() {
    $('#navi').click(function() {
        $('.posts').fadeOut();
        $('.muses').fadeOut();
        $('.tagsnav').fadeIn();
    });
    
    $('.tagsnav .close').click(function() {
        $('.tagsnav').fadeOut();
        $('.posts').fadeIn();
    });
});
</script>
