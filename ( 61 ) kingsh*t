<script>
    $(document).ready(function() {      
    $('#pop').click(function() {
        $('#pop').fadeOut();
        $('#entries').fadeOut();
        $('#posts').fadeOut();
        $('#contain').fadeIn();
        
    });
    
    $('#contain .exit').click(function() {
        $('#contain').fadeOut();
        $('#posts').fadeIn();
        $('#entries').fadeIn();
        $('#pop').fadeIn();
    });
 });  
</script>


