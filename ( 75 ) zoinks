<script>
$(document).ready(function() {
    $('#trigger').click(function() {
        $('#container').fadeOut();
        $('article').fadeOut();
        $('#sidebar').fadeOut();
        $('.popup').fadeIn();
        $('#pop').fadeIn();
    });
    
    $('.popup .close').click(function() {
        $('.popup').fadeOut();
        $('#pop').fadeOut();
        $('#container').fadeIn();
        $('#sidebar').fadeIn();
        $('article').fadeIn();
    });
});
</script>

