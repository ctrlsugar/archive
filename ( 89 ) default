<script>
      var $container = $('#container');
   $container.find('.perma').each(function(){
       var n = $(this).html().split(' ')[0].replace(/,/g, '');
       if (n > 999) {
           n = Math.floor(n / 100) / 10;
           $(this).text(n + 'k');
       }
   });
</script>


